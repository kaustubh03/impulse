{
    "manifest_version": 2,
    "name": "Quotinator",
    "version": "0.1",
    "chrome_url_overrides":{
        "newtab":"index.html"
    },
    "permissions":["tabs"],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}